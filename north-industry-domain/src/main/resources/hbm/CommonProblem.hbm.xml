<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.model.CommonProblem" table="common_problem" schema="north_industry">
        <id name="cpId" column="cp_id"/>
        <property name="cpTitle" column="cp_title"/>
        <property name="cpContent" column="cp_content"/>
        <property name="cpAdminId" column="cp_admin_id"/>
        <property name="valid" column="valid"/>
    </class>
</hibernate-mapping>