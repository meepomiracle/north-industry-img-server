<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.model.CategoryConfUnit" table="category_conf_unit" schema="north_industry">
        <id name="ccuId" column="ccu_id"/>
        <property name="ccuItem" column="ccu_item"/>
        <property name="ccuSortOrder" column="ccu_sort_order"/>
        <property name="valid" column="valid"/>
    </class>
</hibernate-mapping>